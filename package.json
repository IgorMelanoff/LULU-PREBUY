{
  "name": "playwright-typescript-playwright-test",
  "version": "1.1.0",
  "description": "Playwright Framework with Typescript",
  "main": "index.ts",
  "scripts": {
    "test": "npx ts-node ./lib/Runner.ts",
    "execute": "npx playwright test",
    "test:single": "npx playwright test Login.test.ts --project=Chrome",
    "test:parallel": "npx playwright test --grep '@API|@Smoke' --workers=3 --project=Chrome",
    "test:api": "npx playwright test --grep @API --project=Chrome",
    "test:serial": "npx playwright test --grep @Smoke --workers=1 --project=Chrome",
    "test:ui": "npx playwright test --grep @Smoke --ui",
    "test:record": "npx playwright codegen",
    "test:visual": "npx playwright test visualComparision.test.ts --project=Chrome",
    "test:device": "npx playwright test Emulation.test.ts --project=Device",
    "allureReport": "npx allure generate ./test-reports/allure-results --clean --single-file -o ./test-reports/allure-report",
    "upload:tr:results": "dotenv -- cross-var trcli -n -h %TR_HOST% -u %TR_USERNAME% -p %TR_API_KEY% --project %TR_PROJECT_NAME% --project-id 234 parse_junit --title %TR_TITLE% --case-matcher name --allow-ms --run-description yes -f './test-results/junit-report.xml'",
    "upload:tr:uf:results": "dotenv -- cross-var trcli -n -h %TR_HOST% -u %TR_USERNAME% -p %TR_API_KEY% --project %TR_UF_PROJECT_NAME% parse_junit --title %TR_UF_TITLE% --case-matcher name --allow-ms --run-description yes -f './test-results/junit-report.xml'",
    "upload:tr:ser:results": "dotenv -- cross-var trcli -n -h %TR_HOST% -u %TR_USERNAME% -p %TR_API_KEY% --project %TR_UF_PROJECT_NAME% parse_junit --title %TR_UF_TITLE% --case-matcher name --allow-ms --run-description yes -f './test-results/junit-report.xml'",
    "preallureReport": "copyfiles -f ./test-reports/allure-report/index.html ./test-reports/history/B2B-NP-Report_$(date \"+%Y-%m-%d\") && find ./test-reports/history/* -type d -ctime +90 | xargs rm -r"
  },
  "author": "Vipul Goyal vgoyal2@lululemon.com",
  "license": "Lululemon B2B Test Framework",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.873.0",
    "@types/adm-zip": "^0.5.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node-fetch": "^2.6.7",
    "adm-zip": "^0.5.10",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "exceljs": "^4.3.0",
    "jest-playwright-preset": "^4.0.0",
    "lighthouse": "^9.6.8",
    "npm": "^9.6.4",
    "oauth-1.0a": "^2.2.6",
    "pdfjs-dist-es5": "^2.13.216",
    "pg": "^8.11.1",
    "playwright": "^1.38.1",
    "python": "^0.0.4",
    "winston": "^3.9.0"
  },
  "jest": {
    "preset": "jest-playwright-preset"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.585.0",
    "@aws-sdk/client-sso-admin": "^3.521.0",
    "@aws-sdk/lib-dynamodb": "^3.585.0",
    "@aws-sdk/util-dynamodb": "^3.577.0",
    "@faker-js/faker": "^8.2.0",
    "@kafkajs/confluent-schema-registry": "^3.3.0",
    "@playwright/test": "^1.53.2",
    "@types/base-64": "^1.0.2",
    "@types/jsonpath": "^0.2.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "allure-commandline": "^2.25.0",
    "allure-playwright": "^2.10.0",
    "aws-sdk": "^2.1565.0",
    "base-64": "^1.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.5",
    "cross-var": "^1.1.0",
    "csv-parse": "^5.5.2",
    "csv-writer": "^1.6.0",
    "deep-equal": "^2.2.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "ioredis": "^5.3.2",
    "jsonpath": "^1.1.1",
    "jsonpath-plus": "^10.3.0",
    "kafkajs": "^2.2.4",
    "mailinator-client": "^1.0.3",
    "mkdirp": "^3.0.1",
    "node-fetch": "^2.7.0",
    "pdf-parse": "^1.1.1",
    "prettier": "^3.1.0",
    "redis": "^4.6.14",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
